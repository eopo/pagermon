{
    "extends": [
        "wesbos"
    ],
    "rules": {
        "no-console": 0,
        "no-unused-expressions": 0,
        "camelcase": 1,
        "no-use-before-define": 0,
        "no-var": 0, // Disable for now! 
        "import/order": 0,
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 120,
                "tabWidth": 8,
            }
        ]
    }
}